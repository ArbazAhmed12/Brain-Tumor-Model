umpy(), prob_map.cpu().numpy()\n","\n","\n","def overlay_mask(image, mask):\n","    image = (image * 255).astype(np.uint8)\n","    image_rgb = cv2.cvtColor(image, cv2.COLOR_GRAY2RGB)\n","\n","    overlay = image_rgb.copy()\n","    overlay[mask == 1] = [255, 0, 0]\n","\n","    blended = cv2.addWeighted(image_rgb, 0.7, overlay, 0.3, 0)\n","    return blended\n","\n","\n","def confidence_heatmap(prob_map):\n","    prob_map = (prob_map * 255).astype(np.uint8)\n","    heatmap = cv2.applyColorMap(prob_map, cv2.COLORMAP_JET)\n","    return heatmap\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"eXgIJKLm4NN4","executionInfo":{"status":"ok","timestamp":1766482488735,"user_tz":-330,"elapsed":287,"user":{"displayName":"ams","userId":"16908876361288379684"}},"outputId":"24e49bde-8393-4dbc-8899-ae3260b7a4ba"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Overwriting utils.py\n"]}]},{"cell_type":"code","source":["# model.py\n","%%writefile model.py\n","# Unet model\n","import torch\n","import torch.nn as nn\n","\n","class UNet(nn.Module):\n","    def __init__(self):\n","        super().__init__()\n","\n","        def block(in_c, out_c):\n","            return nn.Sequential(\n","                nn.Conv2d(in_c, out_c, 3, padding=1),\n","                nn.ReLU(inplace=True),\n","                nn.Conv2d(out_c, out_c, 3, padding=1),\n","                nn.ReLU(inplace=True)\n","            )\n","\n","        self.enc1 = block(1, 64)\n","        self.enc2 = block(64, 128)\n","        self.enc3 = block(128, 256)\n","\n","        self.pool = nn.MaxPool2d(2)\n","\n","        self.up = nn.Upsample(scale_factor=2, mode=\"bilinear\", align_corners=True)\n","\n","        self.dec2 = block(256 + 128, 128)\n","        self.dec1 = block(128 + 64, 64)\n","\n","        self.final = nn.Conv2d(64, 1, kernel_size=1)\n","\n","    def forward(self, x):\n","        e1 = self.enc1(x)\n","        e2 = self.enc2(self.pool(e1))\n","        e3 = self.enc3(self.pool(e2))\n","\n","        d2 = self.up(e3)\n","        d2 = self.dec2(torch.cat([d2, e2], dim=1))\n","\n","        d1 = self.up(d2)\n","        d1 = self.dec1(torch.cat([d1, e1], dim=1))\n","\n","        return torch.sigmoid(self.final(d1))\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"fI2thaSL4SrS","executionInfo":{"status":"ok","timestamp":1766482503212,"user_tz":-330,"elapsed":273,"user":{"displayName":"ams","userId":"16908876361288379684"}},"outputId":"a343071b-7b4a-41c0-e137-201707f3bbaf"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Overwriting model.py\n"]}]},{"cell_type":"code","source":["# app.py\n","%%writefile app.py\n","import streamlit as st\n","import torch\n","import numpy as np\n","from PIL import Image\n","\n","from utils import load_model, predict, overlay_mask, confidence_heatmap\n","\n","MODEL_PATH = \"/content/drive/MyDrive/Major_project/streamlit_app/unet_model.pth\"\n","\n","st.set_page_config(page_title=\"Brain Tumor Detection\", layout=\"wide\")\n","st.title(\"Brain Tumor Detection & Segmentation\")\n","\n","model = load_model(MODEL_PATH)\n","\n","uploaded_file = st.file_uploader(\"Upload MRI Scan\", type=[\"png\", \"jpg\", \"jpeg\"])\n","\n","if uploaded_file:\n","    image = Image.open(uploaded_file).convert(\"L\")\n","    image = image.resize((256, 256))\n","\n","    image_np = np.array(image) / 255.0\n","    image_tensor = torch.tensor(image_np).unsqueeze(0).unsqueeze(0).float()\n","\n","    mask, prob_map = predict(model, image_tensor)\n","\n","    mask = mask.squeeze()\n","    prob_map = prob_map.squeeze()\n","\n","    st.subheader(\"Original MRI\")\n","    st.image(image, width=250)\n","\n","    st.subheader(\"Segmentation Mask\")\n","    st.image(mask, width=250)\n","\n","    overlay = overlay_mask(image_np, mask)\n","    st.subheader(\"Tumor Location Overlay\")\n","    st.image(overlay, use_column_width=True)\n","\n","    heatmap = confidence_heatmap(prob_map